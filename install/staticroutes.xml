<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
	<routes>
		<route>
			<name>Atom feed</name>
			<pattern>|^/feed\/atom/?$|</pattern>
			<reverse>/feed/atom</reverse>
			<module>Feed</module>
			<controller>Feed</controller>
			<action>atom</action>
			<variables></variables>
			<priority>1</priority>
		</route>
		<route>
			<name>RSS feed</name>
			<pattern>|^/feed\/rss/?$|</pattern>
			<reverse>/feed/rss</reverse>
			<module>Feed</module>
			<controller>Feed</controller>
			<action>rss</action>
			<variables></variables>
			<priority>1</priority>
		</route>
	</routes>
</zend-config>
